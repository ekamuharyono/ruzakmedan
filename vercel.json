{
  "version": 2,
  "builds": [
    {
      "src": "api/*.php",
      "use": "@vercel/php"
    },
    {
      "src": "public/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/menu",
      "dest": "api/index.php"
    },
    {
      "src": "/styles.css",
      "dest": "/public/styles.css"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
}